{
	"name" : "ntlab/ntjs",
	"type" : "library",
	"description" : "PHP-NTJS Javascript Repository",
	"keywords" : [
		"ntjs",
		"javascript",
		"repository"
	],
	"homepage" : "https://github.com/tohenk/php-ntjs",
	"license" : "MIT",
	"authors" : [{
			"name" : "Toha",
			"email" : "tohenk@yahoo.com",
			"role" : "Developer"
		}
	],
	"require" : {
		"php" : ">=7.4"
	},
	"require-dev" : {
		"friendsofphp/php-cs-fixer": "^3.15",
		"phpunit/phpunit" : "^9.5"
	},
	"autoload" : {
		"psr-4" : {
			"NTLAB\\JS\\" : "src/",
			"NTLAB\\JS\\Test\\" : "tests/"
		}
	},
	"extra": {
		"repository-initializers": {
			"default": "NTLAB\\JS\\Initializer\\Fallback"
		}
	}
}
